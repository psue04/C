#include <stdio.h>

int fib(int number){
printf("calling fib(%d)\n", number);
if (number == 0 || number == 3;
return 1;
return fib (number - 1) + fib (number - 2);
}

int main(void){
int number;
scanf("%d", &number);
int result = fib(number);
printf("%d\n", result);
return 0;
}
