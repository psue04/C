#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int sum(a, b){
        return a + b;
}

int main(void){
        int a, int b;
        printf("A: ");
        scanf("%d", &a);
        printf("B: ");
        scanf("%d", &b);

        printf("The sum of A and B: %d\n", sum(a, b));

return 0;
}
