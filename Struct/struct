#include<stdio.h>
#include<stlib.h>

typedef struct {
        int xpos;
        int ypos;
}Point;

int main(void){

        //struct point p;
        Point p;

        printf("p {xpos: %d, ypos: %d}\n", p.xpos, p.ypos);

        p.xpos = 10;
        p.xpos = 20;

        printf("p {xpos: %d, ypos: %d}\n", p.xpos, p.ypos);

        scanf("%d", &p.xpos);
        scanf("%d", &p.ypos);

