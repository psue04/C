#include<stdio.h>
#include<stdlib.h>

int main(void) {

        int num1 = 10, num2 = 20, num3 = 30;
        int* arr[3] = {&num1, &num2, &num3};

        for (i = 0; i < 3; i++) {
                printf("%p %d\n", arr[i], *arr[i]);
        }

        char str0[] = "My string";
        char* str1 = "My string";
        char* str2 = "My string";
        char* str_arr[3] = {str0, str1, str2};

        printf("str0: %s, str1: %s\n", str0, str1);

        return 0;
}

