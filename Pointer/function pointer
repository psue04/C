#include<stdio.h>
#include<stdlib.h>

int add(int a, int b) {

        return a + b;
}

int main(void) {
        int a = 10, b = 20;
        int (*fptr)(int, int);
        fptr = add;

        printf("%d\n", add(a, b));
        printf("%d\n", fptr(a, b));

        return 0;
}
